{
  "name": "tsukulive-lighting-commander",
  "version": "0.0.1",
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "description": "Lighting controller",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node dist/main/main/index.js",
    "build": "run-p build:*",
    "build:main": "tsc",
    "build:renderer": "webpack",
    "dev": "run-p dev:*",
    "dev:main": "ts-node-dev --project ./tsconfig.json src/main/index.ts",
    "dev:renderer": "npm run build:renderer -- --watch",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --ext .js,.ts,.vue --fix .",
    "format": "prettier --write \"**/*.{js,ts,json,md,vue,yaml,yml}\""
  },
  "dependencies": {
    "@nandenjin/alien-core": "^0.0.2",
    "@nandenjin/alien-ui": "^0.1.3",
    "artnet": "^1.4.0",
    "assert": "^2.0.0",
    "axios": "^1.6.7",
    "consola": "^3.2.2",
    "express": "^4.17.1",
    "path-browserify": "^1.0.1",
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0",
    "vue": "^2.5.16",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@webpack-cli/serve": "^2.0.0",
    "chalk": "^5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.15.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "=8.0.3",
    "lint-staged": ">=10",
    "nexe": "^4.0.0-rc.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "^1.63.4",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "url-loader": "^4.0.0",
    "vue-eslint-parser": "^9.3.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.11.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-hot-middleware": "^2.22.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*.{js,ts,json,md,vue,yaml,yml}": "prettier --write"
  }
}
